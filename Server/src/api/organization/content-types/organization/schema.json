{
  "kind": "collectionType",
  "collectionName": "organizations",
  "info": {
    "singularName": "organization",
    "pluralName": "organizations",
    "displayName": "organization"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "micro_1_10",
        "small_11_50",
        "medium_51_200",
        "large_201_500",
        "xlarge_501_1000",
        "enterprise_1001_plus"
      ],
      "required": true
    },
    "industry": {
      "type": "enumeration",
      "enum": [
        "technology",
        "finance",
        "healthcare",
        "education",
        "manufacturing",
        "retail",
        "other"
      ],
      "required": true
    },
    "description": {
      "type": "text"
    },
    "avatarUrl": {
      "type": "string"
    },
    "websiteUrl": {
      "type": "string"
    },
    "notificationPreferences": {
      "type": "json"
    },
    "aiGenerationQuota": {
      "type": "integer",
      "default": 500
    },
    "aiGenerationUsage": {
      "type": "integer",
      "default": 0
    },
    "ownerId": {
      "type": "string",
      "required": true
    },
    "members": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::organization-member.organization-member",
      "mappedBy": "organization"
    },
    "invites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::organization-invite.organization-invite",
      "mappedBy": "organization"
    },
    "sharedTemplates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::organization-template.organization-template",
      "mappedBy": "organization"
    },
    "activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::organization-activity.organization-activity",
      "mappedBy": "organization"
    },
    "apiKeys": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::organization-api-key.organization-api-key",
      "mappedBy": "organization"
    },
    "auditLogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::organization-audit-log.organization-audit-log",
      "mappedBy": "organization"
    },
    "apps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::app.app",
      "mappedBy": "organization"
    },
    "supportTickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::support-ticket.support-ticket",
      "mappedBy": "organization"
    }
  }
}
