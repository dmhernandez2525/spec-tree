{
  "kind": "collectionType",
  "collectionName": "user_stories",
  "info": {
    "singularName": "user-story",
    "pluralName": "user-stories",
    "displayName": "UserStory",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "position": {
      "type": "integer",
      "default": 0
    },
    "goal": {
      "type": "text"
    },
    "developmentOrder": {
      "type": "integer"
    },
    "role": {
      "type": "string"
    },
    "actionStr": {
      "type": "string"
    },
    "notes": {
      "type": "string"
    },
    "points": {
      "type": "integer"
    },
    "tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.task",
      "mappedBy": "userStory"
    },
    "feature": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::feature.feature",
      "inversedBy": "userStories"
    },
    "contextualQuestions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::contextual-question.contextual-question",
      "mappedBy": "userStories"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "userStory"
    },
    "acceptanceCriteria": {
      "type": "component",
      "repeatable": true,
      "component": "micro-component.acceptance-criteria"
    }
  }
}
